{
  "name": "@intra365/outlook-navigation",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@intra365/models": "^1.0.0",
    "@reach/router": "^1.2.1",
    "core-js": "^2.6.1",
    "es6-promise": "^4.2.6",
    "office-ui-fabric-react": "^6.187.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "office-addin-debugging": "^2.1.13",
    "office-addin-dev-certs": "^1.0.1",
    "office-toolbox": "^0.1.1"
  },
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "debug": "office-addin-debugging start outlook-navigation-manifest.xml",
    "sideload": "office-toolbox sideload -m outlook-navigation-manifest.xml -a outlook",
    "start:desktop": "office-addin-debugging start outlook-navigation-manifest.xml desktop",
    "start:web": "office-addin-debugging start outlook-navigation-manifest.xml web",
    "stop": "office-addin-debugging stop outlook-navigation-manifest.xml",
    "unload": "office-toolbox remove -m outlook-navigation-manifest.xml -a outlook",
    "validate": "office-toolbox validate -m outlook-navigation-manifest.xml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
