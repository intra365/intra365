  "main": "public/electron.js",
  "scripts": {
    "analyse": "source-map-explorer build/static/js/main.*",
    "start": "HTTPS=false react-scripts start",
    "sideload": "office-toolbox sideload -m pto-365-word-manifest.xml -a Word",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "injectbuildnumber": "node src/_build",
    "test": "react-scripts test --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "test:ci": "react-scripts test --env=jsdom --reporters=default --reporters=jest-junit ",
    "test:ciorig": "react-scripts test --env=jsdom --reporters=default --reporters=jest-junit  --coverage --coverageDirectory=test/coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "screenshot": "storybook-chrome-screenshot -p 9009 -c .storybook",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "release": "standard-version",
    "electron": "electron .",
    "electron-dev": "ELECTRON_START_URL=https://localhost:3000 electron .",
    "pack": "electron-builder --win",
    "flow":"flow"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-info": "^3.3.14",
    "@storybook/addon-knobs": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "cypress": "^3.0.2",
    "electron": "^3.0.2",
    "electron-builder": "^20.18.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-matchers": "^6.0.2",
    "enzyme-to-json": "^3.3.4",
    "foreman": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-junit": "^5.1.0",
    "jest-localstorage-mock": "^2.2.0",
    "office-addin-validator": "^1.0.5",
    "office-toolbox": "^0.1.0",
    "react-styleguidist": "^7.0.20",
    "redux-devtools": "^3.5.0",
    "sinon": "^6.1.3",
    "standard-version": "^4.4.0",
    "storybook-chrome-screenshot": "^0.6.1"
  },
  "build": {
    "appId": "com.jumpto365.app",
    "compression": "normal",
    "productName": "jumpto365",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "mac": {
      "icon": "assets/icon.icns",
      "type": "distribution",
      "target": [
        "pkg",
        "dmg",
        "mas"
      ],
      "publish": {
        "provider": "s3",
        "bucket": "example-releases",
        "path": "releases"
      },
      "category": "public.app-category.news"
    },
    "mas": {
      "entitlements": "assets/entitlements.mas.plist",
      "entitlementsInherit": "assets/entitlements.mas.inherit.plist",
      "provisioningProfile": "assets/embedded.provisionprofile"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico",
      "publish": {
        "provider": "s3",
        "bucket": "example-releases",
        "path": "releases"
      }
    },
    "linux": {
      "icon": "assets/icon.png",
      "target": [
        "snap",
        "AppImage"
      ],
      "description": "jumpto365",
      "category": "Network;Feed",
      "publish": {
        "provider": "s3",
        "bucket": "example-releases",
        "path": "releases"
      }
    }
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "test/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
